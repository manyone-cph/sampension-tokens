{
  "name": "@manyone/sampension-tokens",
  "version": "1.0.2",
  "description": "Design tokens for the Sampension Design System",
  "private": false,
  "license": "ISC",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/manyone-cph/sampension-tokens.git"
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "build-json:shared": "npx token-transformer tokens.json dist/json/shared.json shared/core/typography,shared/core/color,shared/core/dimension,shared/semantic/typography,shared/semantic/color,shared/semantic/dimension,shared/semantic/border,shared/composition/typography,shared/composition/atoms,shared/composition/molecules,shared/composition/layout --expandTypography=true --expandComposition=true",
    "build-js-scss:shared": "node build/build.styles.mjs",
    "build": "npm run clean && npm run build-json:shared && npm run build-js-scss:shared"
  },
  "publishConfig": {
    "@manyone:registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "node-fetch": "^3.2.0",
    "token-transformer": "^0.0.29"
  },
  "devDependencies": {
    "semantic-release": "^20.1.0",
    "style-dictionary": "^3.7.2"
  }
}